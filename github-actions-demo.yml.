
name: Unzip File

on:
  push:
    branches:
      - main  # Atau branch lain yang Anda gunakan
  pull_request:
    branches:
      - main  # Atau branch lain yang Anda gunakan

jobs:
  unzip:
    runs-on: ubuntu-latest

    steps:
      - name: Checkout repository
        uses: actions/checkout@v2

      - name: Unzip file
        run: |
          unzip path/to/cihuy.zip -d path/to/destination/
